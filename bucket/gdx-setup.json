{
    "version": "1.0",
    "description": "Gdx-Setup is a utility to create projects with the LibGdx library and some other compatible utilities",
    "homepage": "https://libgdx.com",
    "license": "MIT",
    "suggest": {
        "JDK": [
            "java/openjdk"
        ]
    },
    "shortcuts": [
        [
            "gdx-setup.bat",
            "gdx-setup"
        ]
    ],
    "url": " https://libgdx-nightlies.s3.amazonaws.com/libgdx-runnables/gdx-setup.jar",
    "pre_install": [
        "Set-Content \"$dir\\gdx-setup.bat\" '@start java.exe -jar \"%~dp0gdx-setup.jar\"'"
    ],
    "bin": "gdx-setup.bat"
}
